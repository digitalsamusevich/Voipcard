				{% else %}
				  <a href="{{ home }}"><img {% if (logo_iwh_status) %}width="{{ logo_w }}" height="{{ logo_h }}"{% endif %} src="{{ logo }}" title="{{ name }}" alt="{{ name }}" class="img-responsive" /></a>
				{% endif %}
			  {% else %}
				<h1><a href="{{ home }}">{{ name }}</a></h1>
			  {% endif %}
			</div>
		</div>
		{% if (type_header == 2) %}
			{% if (header_doptext is not empty) %}
				<div class="header_dopmenu_doptext ml-auto hidden-xs{% if (swap_header_blocks is defined and (swap_header_blocks == '1')) %} f-order-3{% else %} f-order-2{% endif %}">{{ header_doptext }}</div>
			{% endif %}
		{% endif %}
		{% set class_phone = '' %}
