		{% if ((type_header == 2) and (swap_search_dmenu == 0) and (config_main_menu_selection == 1) or ((type_header == 2) and (swap_search_dmenu == 0) or (swap_search_dmenu == 1) and (config_main_menu_selection == 0)) ) %}
		<div class="dop-menu-header visible-md visible-lg col-xs-12 col-sm-12 col-md-6 col-lg-6 ml-auto {% if (swap_header_blocks is defined and (swap_header_blocks == '1')) %}f-order-2 dopmenu-center{% else %}ml-auto f-order-3{% endif %}">
			{{ cs_dop_menu }}
		</div>
		{% endif %}
		{% if ((type_header == 1) or (type_header == 2) and (swap_search_dmenu == 1) and (config_main_menu_selection == 1)) %}
