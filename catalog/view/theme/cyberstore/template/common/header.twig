  {% if (header_nav_menu_links is not empty) %}
	<div class="pull-left">
		<div class="btn-group box-question">
			<button data-toggle="dropdown" type="button" class="visible-xs visible-sm btn btn-link"><i class="fa fa-bars" aria-hidden="true"></i></button>
			<ul class="dropdown-menu dropdown-menu-left header-nav-links cs-dropdown">
				{% for header_nav_menu_link in header_nav_menu_links %}
					<li><button {% if ((header_nav_menu_link['popup'] == '1') and (header_nav_menu_link['link'] != '')) %} onclick="$('.side-collapse').toggleClass('in'); banner_link_open('{{ header_nav_menu_link['link'] }}')" {% elseif ((header_nav_menu_link['popup'] == '0') and (header_nav_menu_link['link'] != '')) %}onclick="location='{{ header_nav_menu_link['link'] }}'" {% endif %} type="button" class="btn btn-link">{% if (header_nav_menu_link['icon_image'] is not empty) %}<i class="{{ header_nav_menu_link['icon_image'] }}"></i> {% endif %}{{ header_nav_menu_link['title'][lang_id] }}</button></li>
				 {% endfor %}
			</ul>
		</div>
	</div>
	{% endif %}
	<div class="pull-right">
		{% if (type_header != 2) %}
		<div class="pull-right btn-group box-account">
				<button data-toggle="dropdown" type="button" class="dropdown-toggle btn btn-link">
				{% if (logged) %}
				{{ '<b>'~first_name~' '~last_name~'</b>' }}
				{% else %}
				<span class="hidden-xs hidden-sm">{{ text_account }}</span></span><svg xmlns="http://www.w3.org/2000/svg" width="7" height="4" fill="none" viewBox="0 0 7 4"><path stroke="#000" stroke-linecap="round" d="M6 1.1L4.15 2.597a1 1 0 01-1.25.007L1 1.1"/></svg>
				{% endif %}
				</button>
			<ul class="dropdown-menu dropdown-menu-right cs-dropdown">
				{% if (logged) %}
				<li><a rel="nofollow" href="{{ account }}">{{ text_account }}</a></li>
				<li><a rel="nofollow" href="{{ order }}">{{ text_order }}</a></li>
				<li><a rel="nofollow" href="{{ transaction }}">{{ text_transaction }}</a></li>
				<li><a rel="nofollow" href="{{ download }}">{{ text_download }}</a></li>
				<li><a rel="nofollow" href="{{ logout }}">{{ text_logout }}</a></li>
				{% else %}
				<li><a rel="nofollow" href="{{ register }}">{{ text_register }}</a></li>
				<li><a rel="nofollow" href="javascript:void(0)" id="login-popup" data-load-url="index.php?route=common/login_modal">{{ text_login }}</a></li>
				{% endif %}
			</ul>
		</div>
		{% endif %}
		<div class="pull-right box-currency">{{ currency }}</div>
		<div class="pull-right box-language">{{ language }}</div>
  </div>
  </div>
</nav>
<header class="h-type-{{ type_header }} {% if (main_menu_fix_mobile is defined and (main_menu_fix_mobile == 1)) %} pad-top{% endif %}">
  <div class="container h-box">
    <div class="row-flex vertical-align">
