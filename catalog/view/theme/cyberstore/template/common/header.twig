			{% set class_phone = class_phone ~'col-xs-6 col-sm-8 col-md-6 push-right' %}
			{% if (swap_header_blocks is defined and (swap_header_blocks == '1')) %}
				{% set class_phone = class_phone ~' f-order-3' %}
			{% else %}
				{% set class_phone = class_phone ~' f-order-2' %}
			{% endif %}
		{% endif %}
