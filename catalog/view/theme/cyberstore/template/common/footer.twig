                <i class="{{ rsf['social_icon_footer'] }}"></i>
                {% else %}
                <img src="{{ rsf['ifs'] }}" alt="" title="">
                {% endif %}
                </a>
            {% endfor %}
            </li>
        </ul>
        </div>
