	     	{% if (type_header == 1) %}
	     	{% set class_phone = class_phone ~' text-xs-right' %}
	     	{% endif %}
	     	{% set class_phone = class_phone ~' text-sm-right' %}
	    {% else %}
	     	{% if (type_header == 1) %}
	     	{% set class_phone = class_phone ~' col-xs-6 col-sm-4 text-sm-left' %}
	     	{% endif %}
	     	{% if (type_header == 2) %}
	     	{% set class_phone = class_phone ~' col-sm-6 text-sm-right' %}
	     	{% endif %}
	     	{% set class_phone = class_phone ~' text-xs-left text-md-center' %}
	     {% endif %}

