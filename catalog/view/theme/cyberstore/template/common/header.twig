									<div class="icon-image"><img src="{{ result_hp['icon_phone'] }}" alt="{{ result_hp['phone'] }}" /></div>
									{% elseif (result_hp['icon'] != '') %}
									<div class="icon-image"><i class="{{ result_hp['icon'] }}"></i></div>
								{% endif %}
								{{ result_hp['phone']}}</a>
							{% else %}
								{% if (result_hp['icon_phone'] != '') %}
									<div class="icon-image"><img src="{{ result_hp['icon_phone'] }}" alt="{{ result_hp['phone'] }}" /></div>
									{% elseif (result_hp['icon'] != '') %}
									<div class="icon-image"><i class="{{ result_hp['icon'] }}"></i></div>
								{% endif %}
								{{ result_hp['phone']}}
							{% endif %}
							</div>
