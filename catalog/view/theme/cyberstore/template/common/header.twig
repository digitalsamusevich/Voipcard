						<li class="info-contact">
							{{ arbitrary_text }}
						</li>
						<li class="line-contact"></li>
						{% endif %}
						{% if (header_phones_dropdown is not empty) %}
							{% for result_ddh in header_phones_dropdown %}
								<li>
								{% if (result_ddh['type'] is not empty) %}
									<a {{ result_ddh['type']}}>
										{% if (result_ddh['icon_phone'] != '') %}
										<div class="icon-image"><img src="{{ result_ddh['icon_phone'] }}" alt="{{ result_ddh['phone'] }}" /></div>
										{% elseif (result_ddh['icon'] != '') %}
										<div class="icon-image"><i class="{{ result_ddh['icon'] }}"></i></div>
										{% endif %}
										{{ result_ddh['phone'] }}
									</a>
									{% else %}
										<div>
										{% if (result_ddh['icon_phone'] != '') %}
											<div class="icon-image"><img src="{{ result_ddh['icon_phone'] }}" alt="{{ result_ddh['phone'] }}" /></div>
											{% elseif (result_ddh['icon'] != '') %}
											<div class="icon-image"><i class="{{ result_ddh['icon'] }}"></i></div>
											{% endif %}
											{{ result_ddh['phone'] }}
										</div>
									{% endif %}
								</li>
							{% endfor %}
